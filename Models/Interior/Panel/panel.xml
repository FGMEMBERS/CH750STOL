<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

<!--Throttle-->
 <animation>
   <type>translate</type>
   <object-name>throttle</object-name>
   <property>controls/engines/engine[0]/throttle</property>
   <interpolation>
     <entry><ind> 0 </ind><dep>  0.00 </dep></entry>
     <entry><ind> 1 </ind><dep> -0.08 </dep></entry>
   </interpolation>
   <axis>
    <x> 1 </x>
    <y> 0 </y>
    <z> 0 </z>
   </axis>
  </animation>
<!--Mixture-->
 <animation>
   <type>translate</type>
   <object-name>mixture</object-name>
   <property>controls/engines/engine[0]/mixture</property>
   <interpolation>
     <entry><ind> 0 </ind><dep>  0.00 </dep></entry>
     <entry><ind> 1 </ind><dep> -0.08 </dep></entry>
   </interpolation>
   <axis>
    <x> 1 </x>
    <y> 0 </y>
    <z> 0 </z>
   </axis>
  </animation>
<!--Propeller Pitch-->
 <animation>
   <type>translate</type>
   <object-name>proppitch</object-name>
   <property>controls/engines/engine[0]/propeller-pitch</property>
   <interpolation>
     <entry><ind> 1 </ind><dep>  0.00 </dep></entry>
     <entry><ind> 0 </ind><dep> 0.05 </dep></entry>
   </interpolation>
   <axis>
    <x> 1 </x>
    <y> 0 </y>
    <z> 0 </z>
   </axis>
  </animation>

<!--Magneto Switch-->
<animation>
  <type>rotate</type>
  <object-name>magnetoswitch</object-name>
  <property>/controls/engines/engine/magnetos</property>
  <factor>60</factor>
  <center>
   <x-m>-0.01913</x-m>
   <y-m>0.03548</y-m>
   <z-m>-0.0183</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
<animation>
  <type>rotate</type>
  <object-name>Ignition.000</object-name>
  <property>/controls/engines/engine/magnetos</property>
  <factor>35</factor>
  <center>
   <x-m>-0.01913</x-m>
   <y-m>0.03548</y-m>
   <z-m>-0.0183</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<!--SWITCHES-->
<!--Alternator switch-->
 <animation>
    <type>pick</type>
    <object-name>alt</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine/master-alt</property>
      </binding>
      <binding>
          <command>property-toggle</command>
          <property>sim/model/sound/click</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>alt</object-name>
    <property>controls/engines/engine/master-alt-pos</property>
    <factor> 40 </factor>
    <center>
      <x-m> -0.02346 </x-m>
      <y-m>-0.31114 </y-m>
      <z-m> -0.0155 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
